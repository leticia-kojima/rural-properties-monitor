{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "Broker": "kafka:9092",
    "Topic": "sensor-data",
    "GroupId": "ingress-api-group"
  },
  "InfluxDbConfig": {
    "Url": "http://localhost:8086",
    "Token": "influxdb-token",
    "Org": "myorg",
    "Bucket": "ingress"
  }
}
